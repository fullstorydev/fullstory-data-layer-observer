{
  "captures": [
    {
      "browser-configuration":     {
        "name": "Chrome 75",
        "os": "Windows",
        "osVersion": "10",
        "browserName": "Chrome",
        "browserVersion": "75.0",
        "resolution": "1024x768",
        "browserstack.console": "verbose"
