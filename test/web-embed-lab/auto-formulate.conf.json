      "sites": [
        {
          "name": "basics",
          "url": "https://dlowel.ngrok.io/test/web-embed-lab/sites/basics/",
          "close-pause": 5,
          "modifiers": [
            {
              "mime-type-selectors": ["text/html"],
              "replacements": [
                {
                  "selector": "<script async=\"true\" src=\"../../../../build/dlo.js\">(?s:.*)</script>",
                  "replacement": "<!-- removed original script -->",
                  "all": true
                }
              ]
